[core]
  autocrlf = false
[push]
  default = simple
[user]
  email = etc@adamball.org
  name = Adam Ball
[pull]
  ff = only
[alias]
  br = branch
  ci = commit
  co = checkout
  last = log -1 HEAD
  st = status
  unstage = reset HEAD --
  fa = fetch --all
  p = pull
  pn = pull --no-recurse-submodules
